def parameters
  time       = 1.0e-4
  systemVol  = 1.25e-19
  outputBuffer = 50000
  outputFreq = 10
end

def molecules
  a = 1000
  b = 1000
  c = 0
end

def reactions
  a + b -> c  | {1e6*10} |
  c -> a + b  | 1e5 |
end

def output
  [ITERATION, TIME, a, b, c]
  outputFile = "reversible.dat"
end
